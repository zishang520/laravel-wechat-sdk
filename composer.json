{
    "name": "luoyy/laravel-wechat-sdk",
    "description": "the wechat api library without framework dependency",
    "require": {
        "php": ">=7.0.0"
    },
    "authors": [{
        "name": "luoyy",
        "email": "zishang520@gmail.com"
    }],
    "keywords": ["wechat", "weixin", "laravel", "lumen"],
    "support": {
        "issues": "https://github.com/zishang520/laravel-wechat-sdk/issues",
        "wiki": "https://github.com/zishang520/laravel-wechat-sdk/wiki",
        "source": "https://github.com/zishang520/laravel-wechat-sdk"
    },
    "autoload": {
        "psr-4": {
            "luoyy\\Wechat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Wechat": " luoyy\\Wechat\\Facades\\Wechat",
            },
            "providers": [
                "luoyy\\Wechat\\Providers\\WechatServiceProvider"
            ]
        }
    }
}